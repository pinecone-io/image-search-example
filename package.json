{
  "name": "pinecone-image-search-example",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "starter",
    "template",
    "node",
    "typescript",
    "llm",
    "image search"
  ],
  "license": "MIT",
  "author": "Roie Schwaber-Cohen",
  "type": "module",
  "main": "./dist/server/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run /build:/app",
    "build:app": "vite --config app/vite.config.ts build",
    "build:server": "vite --config server/vite.config.ts build",
    "dev": "pnpm run /dev:/",
    "dev:app": "vite --force --config app/vite.config.ts",
    "dev:server": "vite --force --config server/vite.config.ts",
    "format": "pnpm run /format:/ && pnpm dlx sort-package-json",
    "format:app": "eslint app --fix",
    "format:server": "eslint server --fix",
    "lint": "pnpm run /lint:/",
    "lint:app": "tsc --p ./app && eslint app",
    "lint:server": "tsc --p ./server && eslint server",
    "start": "node dist/server",
    "test": "pnpm run /test:/",
    "test:app": "vitest run --config app/vite.config.ts  --coverage --retry 5",
    "test:server": "vitest run --config server/vite.config.ts  --coverage --retry 5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@pinecone-database/pinecone": "^1.0.0",
    "@xenova/transformers": "^2.2.0",
    "console-table-printer": "^2.11.1",
    "cross-fetch": "^3.1.6",
    "express": "^4.18.2",
    "msw": "^1.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.7",
    "vite-plugin-node": "^3.0.2",
    "vite-plugin-static-copy": "^0.17.0",
    "vite-plugin-windicss": "^1.9.0",
    "vitest": "^0.34.3",
    "windicss": "^3.5.6"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": "18.17.1",
    "npm": "Please use pnpm 8.6.12",
    "pnpm": "8.6.12",
    "yarn": "Please use pnpm 8.6.12"
  },
  "msw": {
    "workerDirectory": "app/public"
  }
}
